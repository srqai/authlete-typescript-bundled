# authelete-bundled SDK Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# Security Configuration
# The SDK supports loading security credentials from environment variables
# with the prefix: AUTHELETEBUNDLED_
#
# Security environment variables:
AUTHELETEBUNDLED_AUTHLETE=your_authlete_here
AUTHELETEBUNDLED_BEARER=your_bearer_here

# Debug Configuration
# Enable HTTP request/response logging for debugging
AUTHELETEBUNDLED_DEBUG=false
